      "email": "jb@noknot.fr"
    }
  ],
  "license": "MIT",
  "jest": {
    "moduleNameMapper": {
      "^Config$": "<rootDir>/build-config/environments/config.prod.js",
      "\\.(css|scss|png)$": "identity-obj-proxy"
    },
    "testPathIgnorePatterns": [
      "<rootDir>/node_modules/",
      "<rootDir>/jison/",
      "<rootDir>/_book/"
    ],
    "collectCoverageFrom": [
      "<rootDir>/src/**/*.{js,jsx}"
    ]
  },
  "prettier": {
    "singleQuote": true
  }
}
